{
  "manifest_version": 2,

  "name": "ClickBlock",
  "description": "Notifies the user of clickbait articles",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "ClickBlock"
  },

  "content_scripts": [
	 { "matches": ["https://www.facebook.com/*"],
	   "js": ["jquery.js", "underscore.js", "replace.js"],
	   "run_at": "document_end"
	 }
],
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
